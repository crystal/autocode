{
  "name": "@zappjs/next-zapp",
  "version": "0.1.4",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/zapp.cjs",
  "module": "./dist/esm/zapp.js",
  "types": "./dist/types/zapp.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/zapp.d.ts",
      "import": "./dist/esm/zapp.js",
      "require": "./dist/cjs/zapp.cjs"
    }
  },
  "devDependencies": {
    "@types/pluralize": "0.0.29",
    "@zappjs/cli": "^2.0.1",
    "@zappjs/core": "^2.1.1",
    "@zappjs/git": "^0.1.4",
    "@zappjs/handlebars": "^0.2.3",
    "@zappjs/json": "^0.2.1",
    "@zappjs/license": "^0.1.1",
    "@zappjs/prettier": "^0.5.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json && mv dist/cjs/zapp.js dist/cjs/zapp.cjs",
    "dev": "tsc -b --watch ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json && mv dist/cjs/zapp.js dist/cjs/zapp.cjs"
  },
  "dependencies": {
    "@types/node": "^18.0.0",
    "@vercel/postgres-kysely": "^0.5.0",
    "change-case": "^3.1.0",
    "kysely": "^0.26.3"
  }
}