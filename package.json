{
  "private": true,
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "workspace:*",
    "turbo": "^1.10.16"
  },
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate": "turbo run generate",
    "lint": "turbo run lint",
    "parse": "turbo run parse",
    "release": "turbo run build lint test && changeset publish",
    "version": "changeset version && turbo run build --filter @specui/cli"
  },
  "packageManager": "pnpm@9.5.0"
}
